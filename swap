#include <stdio.h>
void swap(int*,int*);
void main()
    {
        int a,b;
        int *p,*q;
        printf("enter the value of a and b");
        scanf("%d%d",&a,&b);
        swap(&a,&b);
        printf("%d%d",a,b);
    }
void swap(int*p,int*q)
    {
      int temp;
      temp=*p;
      *p=*q;
      *q=temp;
      
    }
    
